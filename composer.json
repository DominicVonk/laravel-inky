{
    "name": "rsvpify/laravel-inky",
    "description": "Foundation Inky email templates in Laravel",
    "keywords": ["foundation", "inky", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rsvpify",
            "email": "support@rsvpify.com"
        }
    ],
    "require": {
        "incentfit/inky": "1.3.6.5",
        "illuminate/support": "^6.0",
        "illuminate/view": "^6.0",
        "symfony/css-selector": "^2.7|^3.0|^4.0",
        "symfony/dom-crawler": "^2.7|^3.0|^4.0",
        "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "graham-campbell/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Rsvpify\\LaravelInky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rsvpify\\Tests\\LaravelInky\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rsvpify\\LaravelInky\\InkyServiceProvider"
            ]
        }
    }
}
